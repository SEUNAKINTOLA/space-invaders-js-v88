{
  "name": "space-invaders-js-v88",
  "version": "1.0.0",
  "description": "Modern implementation of Space Invaders game with advanced wave patterns and game engine",
  "main": "src/engine/GameEngine.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "python -m pytest tests/e2e",
    "test:performance": "python -m pytest tests/performance",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "format": "prettier --write \"src/**/*.{js,css}\" \"tests/**/*.js\"",
    "docs": "jsdoc -c jsdoc.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/space-invaders-js-v88.git"
  },
  "keywords": [
    "game",
    "space-invaders",
    "canvas",
    "javascript",
    "python",
    "game-engine"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "core-js": "^3.30.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdoc": "^4.0.2",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "pythonDependencies": {
    "pytest": "^7.3.1",
    "pytest-benchmark": "^4.0.0",
    "pytest-cov": "^4.0.0",
    "black": "^23.3.0",
    "pylint": "^2.17.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/mocks/styleMock.js",
      "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/tests/mocks/fileMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}