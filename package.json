{
  "name": "space-invaders-js-v88",
  "version": "1.0.0",
  "description": "Modern JavaScript implementation of the classic Space Invaders game with advanced wave patterns and game engine",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "python -m pytest tests/e2e",
    "test:performance": "python -m pytest tests/performance",
    "test:perf": "npm run test:performance",
    "benchmark:collision": "node tests/performance/benchmark_collision.js",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,html}\" \"tests/**/*.{js,jsx}\"",
    "prepare": "husky install",
    "docs": "jsdoc -c jsdoc.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SEUNAKINTOLA/space-invaders-js-v88.git"
  },
  "keywords": [
    "game",
    "space-invaders",
    "javascript",
    "canvas",
    "html5",
    "game-engine"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.22.5",
    "core-js": "^3.30.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@testing-library/jest-dom": "^5.16.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsdoc": "^4.0.2",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,html}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "pythonDependencies": {
    "pytest": "^7.3.1",
    "pytest-benchmark": "^4.0.0",
    "pytest-cov": "^4.0.0",
    "black": "^23.3.0",
    "pylint": "^2.17.4"
  }
}